{
  "editor.formatOnSave": true,
  "editor.rulers": [{ "column": 100 }],
  "editor.suggest.localityBonus": true,
  "editor.suggestSelection": "recentlyUsedByPrefix",
  "editor.suggest.shareSuggestSelections": true,
  "editor.suggest.showWords": false,
  "editor.wordWrapColumn": 100,

  "files.associations": {
    "*.project": "ini",
    "*.script": "lua",
    "*.editor_script": "lua",
    "*.gui_script": "lua",
    "*.render_script": "lua",
    "*.fp": "glsl",
    "*.vp": "glsl",
    "*.animationset": "textproto",
    "*.atlas": "textproto",
    "*.camera": "textproto",
    "*.collection": "textproto",
    "*.collectionfactory": "textproto",
    "*.collectionproxy": "textproto",
    "*.collisionobject": "textproto",
    "*.display_profiles": "textproto",
    "*.factory": "textproto",
    "*.gamepads": "textproto",
    "*.go": "textproto",
    "*.gui": "textproto",
    "*.input_binding": "textproto",
    "*.label": "textproto",
    "*.manifest": "textproto",
    "*.material": "textproto",
    "*.mesh": "textproto",
    "*.model": "textproto",
    "*.particlefx": "textproto",
    "*.render": "textproto",
    "*.sound": "textproto",
    "*.spinemodel": "textproto",
    "*.spinescene": "textproto",
    "*.sprite": "textproto",
    "*.texture_profiles": "textproto",
    "*.tilemap": "textproto",
    "*.tilesource": "textproto",
    "*.buffer": "json"
  },

  "glsllint.additionalStageAssociations": {
    ".fp": "frag",
    ".vp": "vert"
  },

  "Lua.runtime.version": "Lua 5.1",
  "Lua.completion.callSnippet": "Replace",
  "Lua.completion.displayContext": 0,
  "Lua.completion.workspaceWord": false,
  "Lua.completion.keywordSnippet": "Both",
  "Lua.diagnostics.globals": [
    "factory",
    "go",
    "gui",
    "hash",
    "json",
    "msg",
    "particlefx",
    "pprint",
    "render",
    "resource",
    "socket",
    "sound",
    "spine",
    "sprite",
    "sys",
    "timer",
    "vmath",
    "window"
  ],
  "Lua.diagnostics.disable": [
    "lowercase-global",
    "missing-parameter",
    "redefined-local",
    "trailing-space"
  ],
  "Lua.workspace.ignoreDir": [
    ".vscode",
    ".internal",
    ".defold_api",
    "build",
    "dist",
    "node_modules",
    "tools"
  ],
  "Lua.workspace.library": [".internal", ".defold_api"]
}
