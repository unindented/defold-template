{
  "editor.formatOnSave": true,
  "editor.rulers": [{ "column": 100 }],
  "editor.suggest.localityBonus": true,
  "editor.suggestSelection": "recentlyUsedByPrefix",
  "editor.suggest.shareSuggestSelections": true,
  "editor.suggest.showWords": false,
  "editor.wordWrapColumn": 100,

  "files.associations": {
    "*.project": "ini",
    "*.script": "lua",
    "*.editor_script": "lua",
    "*.gui_script": "lua",
    "*.render_script": "lua",
    "*.fp": "glsl",
    "*.vp": "glsl",
    "*.animationset": "textproto",
    "*.atlas": "textproto",
    "*.camera": "textproto",
    "*.collection": "textproto",
    "*.collectionfactory": "textproto",
    "*.collectionproxy": "textproto",
    "*.collisionobject": "textproto",
    "*.display_profiles": "textproto",
    "*.factory": "textproto",
    "*.gamepads": "textproto",
    "*.go": "textproto",
    "*.gui": "textproto",
    "*.input_binding": "textproto",
    "*.label": "textproto",
    "*.manifest": "textproto",
    "*.material": "textproto",
    "*.mesh": "textproto",
    "*.model": "textproto",
    "*.particlefx": "textproto",
    "*.render": "textproto",
    "*.sound": "textproto",
    "*.spinemodel": "textproto",
    "*.spinescene": "textproto",
    "*.sprite": "textproto",
    "*.texture_profiles": "textproto",
    "*.tilemap": "textproto",
    "*.tilesource": "textproto",
    "*.buffer": "json"
  },

  "glsllint.additionalStageAssociations": {
    ".fp": "frag",
    ".vp": "vert"
  },

  "Lua.runtime.version": "Lua 5.1",
  "Lua.completion.callSnippet": "Replace",
  "Lua.completion.displayContext": 0,
  "Lua.completion.workspaceWord": false,
  "Lua.completion.keywordSnippet": "Both",
  "Lua.diagnostics.globals": [
    "bit",
    "socket",
    "buffer",
    "pprint",
    "hash",
    "hash_to_hex",
    "collectionfactory",
    "collectionproxy",
    "crash",
    "factory",
    "go",
    "gui",
    "html5",
    "http",
    "image",
    "json",
    "label",
    "model",
    "msg",
    "particlefx",
    "physics",
    "profiler",
    "render",
    "resource",
    "sound",
    "sprite",
    "sys",
    "tilemap",
    "timer",
    "vmath",
    "window",
    "zlib",
    "defos",
    "assert_blank",
    "assert_empty",
    "assert_equal",
    "assert_error",
    "assert_false",
    "assert_greater_than",
    "assert_gte",
    "assert_less_than",
    "assert_lte",
    "assert_match",
    "assert_nil",
    "assert_same",
    "assert_true",
    "assert_type",
    "assert_unique",
    "assert_not_blank",
    "assert_not_empty",
    "assert_not_equal",
    "assert_not_error",
    "assert_not_false",
    "assert_not_greater_than",
    "assert_not_gte",
    "assert_not_less_than",
    "assert_not_lte",
    "assert_not_match",
    "assert_not_nil",
    "assert_not_same",
    "assert_not_true",
    "assert_not_type",
    "assert_not_unique"
  ],
  "Lua.diagnostics.disable": [
    "lowercase-global",
    "missing-parameter",
    "redefined-local",
    "trailing-space"
  ],
  "Lua.workspace.ignoreDir": [
    ".vscode",
    ".internal",
    ".defold_api",
    "build",
    "dist",
    "node_modules",
    "tools"
  ],
  "Lua.workspace.library": [".internal", "${3rd}/Defold/library"]
}
